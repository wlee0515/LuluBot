{
  "startPhase" : "prelaunch",
  "phaseCountlimit" : 10,
  
  "prelaunch" : {

    "processList" : [
      ["git", "pull"],
      ["python", "-m", "pip", "install", "-r" ,"./requirements.txt"]
    ],
    
    "nextPhase" : "launch"
  },
  
  "launch" : {

    "runParallel" : true,
    
    "processList" : [
      ["python", "Lulubot.py"],
      ["python", "webserver.py"]
    ]
    
  }
}